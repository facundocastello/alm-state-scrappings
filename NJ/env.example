# NJ Healthcare Scraper Configuration

# Concurrency Settings
NJ_FACILITY_CONCURRENCY=2  # Number of facilities to scrape in parallel
NJ_REPORT_CONCURRENCY=2    # Number of reports to download in parallel

# Timeout Settings (milliseconds)
NJ_TIMEOUT_MS=60000        # Request timeout (60 seconds default)

# Retry Settings
NJ_RETRY_LIMIT=3           # Max retry attempts per request (3 default)
NJ_RETRY_BACKOFF_LIMIT=5000  # Max delay between retries in ms (5 seconds)

# Progress Tracking
NJ_SKIP_COMPLETED=true     # Skip already-processed facilities (true/false)

# Optional: Custom paths
# NJ_OUTPUT_DIR=./output
# NJ_REPORTS_DIR=./reports
# NJ_DATA_DIR=./data

# Optional: Custom user agent
# NJ_USER_AGENT="Mozilla/5.0 ..."

# Optional: Logging
# NJ_LOG_LEVEL=info
# NJ_LOG_FILE=./nj-scraper.log
